{
  "name": "@personal-search/server",
  "version": "1.0.0",
  "description": "Backend Node.js + TypeScript",
  "main": "dist/server.js",
  "scripts": {
    "dev": "nodemon --exec tsx src/server.ts",
    "dev:tsx": "tsx watch src/server.ts",
